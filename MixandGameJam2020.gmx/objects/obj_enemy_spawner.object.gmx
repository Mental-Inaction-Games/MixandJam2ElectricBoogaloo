                };
                spinner_x = x + 8 + lengthdir_x(8,spinner_angle);
                spinner_y = y + 8 + lengthdir_y(8,spinner_angle);
                if(random(1) &lt; 0.25){
                    instance_create(spinner_x,spinner_y,obj_fx_spawn_spinner);
                };
                respawn_timer--;
