    <event eventtype="8" enumb="64">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if((door_state == "state_waiting")and(instance_exists(obj_player_mia))){
    draw_sprite(spr_arrow,image_index,x,y-12);
}else if(door_state == "state_talking"){
    //draw_set_color(c_black);
   // draw_set_alpha(0);
    //draw_rectangle(0,0,view_wview,view_hview,false);
    draw_set_color(c_white);
    //draw_set_alpha(1);
    
    if!(orderup_has_stopped){
        if(orderup_speed &gt; 0){
            orderup_speed -= orderup_accel;
        }else{
            orderup_has_stopped = true;
        };
    }else{
        if(orderup_speed &lt; 4){
            orderup_speed += orderup_accel;
        };
        if(orderup_x &gt; view_wview){
            game_set_pause(false);
        };
    };
    
    orderup_x += orderup_speed;
    draw_sprite(spr_hud_orderup,0,orderup_x,80);
    draw_text(view_wview-orderup_x*2,96,level_name);
}else if(door_state == "state_hungry"){
    switch(desired_food){
        case spr_cooked_braised_carrot:
            ingredient_one = spr_carrot_drop;
            ingredient_two = spr_honey_drop;
            break;
        
        case spr_cooked_spaghetti:
            ingredient_one = spr_noodle_drop;
            ingredient_two = spr_tomato_drop;
            break;
            
        case spr_cooked_salad:
            ingredient_one = spr_carrot_drop;
            ingredient_two = spr_tomato_drop;
            break;
            
        case spr_cooked_ramen:
            ingredient_one = spr_noodle_drop;
            ingredient_two = spr_egg_drop;
            break;
            
        case spr_cooked_glaized_ham:
            ingredient_one = spr_ham_drop;
            ingredient_two = spr_honey_drop;
            break;
            
        case spr_cooked_breakfast:
            ingredient_one = spr_egg_drop;
            ingredient_two = spr_ham_drop;
            break;
    };

    if(order_alpha &lt; 1){
        order_alpha += 0.05;
    };
    draw_set_alpha(order_alpha);
    draw_sprite(spr_hud_recipe,0,x + 18,y - 24);
    draw_sprite(ingredient_one,0,x + 29,y - 6)
    draw_sprite(ingredient_two,0,x + 50,y - 6)
    draw_sprite(desired_food,0,x + 71,y - 6)
    draw_set_alpha(1);
};
</string>
