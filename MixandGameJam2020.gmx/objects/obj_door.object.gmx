        <arguments>
          <argument>
            <kind>1</kind>
            <string>if((sprite_index == spr_door_eat)or(sprite_index == spr_open)){
    if(food_given == desired_food){
        audio_play_sound(snd_success,10,0);
        if(times_to_feed &lt;= 0){
            with(obj_enemy_spawner){
                can_spawn = false;
            };
            with(obj_enemy){
                willdie = true;
                alarm[1] = room_speed/2;
            };
            instance_destroy();
        };
    }else{
        audio_play_sound(snd_failure,10,0);
    };
    
    door_state = "state_hungry";  
    desired_food = choose(desire_option_1,desire_option_2,desire_option_3,desire_option_4,desire_option_5,desire_option_6);
};
</string>
